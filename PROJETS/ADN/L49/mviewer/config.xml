    <proxy url="" />
    <olscompletion url="https://api-adresse.data.gouv.fr/search" type="ban" />
    <searchparameters bbox="false" localities="true" features="false" />

    <themes>
        <!--Theme is large group of themes or layers directly -->
        <theme name="Phase" collapsed="true" id="phase" icon="copy">
            <group name="Etude d'opportunité" id="etude_opp">
                <layer id="l49:vue_opportunite" name="Opportunités" visible="false" queryable="true" fields="id_opp,lot,cdd,id_prog,id_nro,id_reseau,insee,com_dep,emprise,nom,support,travaux,typ_reseaux,cables, typ_cable,fibres, prev_starr,lg_prev_st, gc_typ_mut,gc_typ_int,longueur,debut_trvx,prog_dsp,moa,moe, moe_bpe, ent_tvx, commentair,statut," aliases="ID opportunité,Lot,Chargé de déploiement, ID programmation,Nom NRO,ID réseau,Code INSEE, Commune de départ,Emprise,Nom,Travaux, Support,Type de réseau, Cables,Type de cables, Fibres, Prévision Starr,Longueur de la prévision Starr, Type de GC mutualisé, Type de GC d'interconnexion, Longueur, Début des travaux,Programmation DSP,MOA,MOE, MOE BPE, Entreprise travaux,Commentaire,Statut," infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:vue_numerisation" name="Numérisation des travaux" visible="false" queryable="true" fields="id_opp,nom,lot,cdd,id_prog,id_nro,id_reseau,statut,emprise,support,travaux,typ_reseau, debut_trvx,prog_dsp,longueur,moa,commentaire, envoi_moe" aliases="ID opportunité,Nom,Lot,Chargé de déploiement, ID programmation,Nom NRO,ID réseau,Statut,Emprise,Support,Travaux,Type de réseau, Début travaux,Programmation DSP,Longueur,MOA,Commentaire, Semaine d'envoi MOE" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            </group>
            <group name="Diagnostique" id="diag"></group>
            <group name="PRO" id="pro"></group>
            <group name="Support" id="support">
                <layer id="l49:vue_chambres_a_creer" name="Chambres ADN à  créeer" visible="false" queryable="true" fields="id_coord,fonction, statut_coord,url" aliases="ID coordination, Fonction, Statut de la coordination,URL" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <!--layer id="l49:vue_chambres_existantes" name="Chambres existantes"  visible="false" queryable="true" fields="id_coord, statut_coord,implant, nature, ref_chambr,code_ch1, url" aliases="ID coordination, Statut de la coordination,Implantation, Nature, Référence, Code CH1, URL" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms"-->
                <layer id="l49:vue_chambres_existantes" name="Chambres existantes" visible="false" queryable="true" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms">
                    <template url="templates/demo.adn.mst" />
                </layer>
            </group>
        </theme>
        <theme name="Réseau ADN" collapsed="true" id="adn" icon="wifi">
            <group name="Découpage" id="decoupage">
                <layer id="l49:nro_znro" name="NRO/ZNRO" visible="false" queryable="true" fields="nro_ref,annee_pr, nb_majic,nb_tech,nb_plu,nb_total, comment" aliases="Nom NRO,Année programmation, Nombre MAJIC, Nombre tech, Nombre plu, Nombre total, Commentaire" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:sro_zsro" name="SRO/ZSRO" visible="false" queryable="true" fields="msro_ref,annee_pr, nb_majic,nb_tech,nb_plu,nb_total, comment" aliases="Nom MSRO,Année programmation, Nombre MAJIC, Nombre tech, Nombre plu, Nombre total, Commentaire" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:adn_programmation" name="Programmation ADN" visible="false" queryable="true" fields="pr,phase,total_pris,prises_uti,cout_prise,communes,epci" aliases="ID programmation, Phase,Nombre de prises total, Prises utilisées, Coût, Commune,EPCI 2012" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:adn_lots_geographiques" name="Lots géographiques" visible="false" queryable="true" fields="label,cdd" aliases="Lot,Chargé de déploiement" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            </group>
            <group name="Déploiement réalisé" id="dep_realises">
                <layer id="l49:vue_cables" name="Cables" visible="false" queryable="true" fields="reference, equipement,soustype,modele,diametre,nbr_tubes,nbr_fibres,statut" aliases="Référence,Equipement,Sous-Type,Modéle,Diamétre,Nombre de tubes, Nombre de fibres,Statut" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:vue_chambres_adn_exist" name="Chambres ADN" visible="false" queryable="true" fields="reference,equipement,modele,address,codeinsee,statut" aliases="Référence,Equipement,Modéle,Adresse,Code INSEE,Statut" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:vue_souterrain" name="Souterrain" visible="false" queryable="true" fields="reference,equipement,modele,fourreaux,statut" aliases="Référence,Equipement,Modéle,Nombre de fourreaux,Statut" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:vue_adr_shl" name="Shelters" visible="false" queryable="true" fields="user_ref, soustype,address,location,codeinsee,statut" aliases="Référence utilisateur,Type,Adresse,Commune,Code INSEE,Statut" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            </group>
            <group name="Déploiement à réaliser" id="dep_futur">
                <layer id="l49:vue_starr_transport" name="Starr transport" visible="false" queryable="true" fields="id_element,id_reseau,support,sites,prises,fibres,fibres_p2p,fibres_pon,fibres_res,cables,longueur,cout,nom_nro" aliases="ID,ID réseau,Support,Sites,Prises,Fibres,Fibres p2p,Fibres pon,Fibres res,Cables,Longueur,Coût,Nom NRO" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
                <layer id="l49:vue_starr_liens" name="Starr lien" visible="false" queryable="true" fields="id_element,id_reseau,support,sites,prises,fibres,fibres_p2p,fibres_pon,fibres_res,cables,longueur,cout,nom_nro" aliases="ID,ID réseau,Support,Sites,Prises,Fibres,Fibres p2p,Fibres pon,Fibres res,Cables,Longueur,Coût,Nom NRO" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            </group>
        </theme>
        <theme name="ERDF" collapsed="true" id="erdf" icon="bolt">
            <layer id="l49:troncon_aerien" name="Tronçons aériens" visible="false" queryable="true" fields="nature,designatio,nom_commun,code_insee" aliases="Nature du tronçon,Désignation, Commune, Code INSEE" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />

        </theme>
        <theme name="Orange" collapsed="true" id="orange" icon="tty">
            <layer id="l49:vue_arciti" name="ArcIti" visible="false" queryable="true" fields="compositio,mode_pose,longueur" aliases="Composition,Mode de pose, Longueur" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:vue_chambres_ft" name="Chambres FT" visible="false" queryable="true" fields="code_ch1,ref_chambr,ref_note,code_com,cle_mkt2,implant" aliases="Num�ro de chambre, Type de chambre, Type de chambre sp�cifique, Code commune, Clé MKT2,Type d'implantation " infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />

        </theme>
        <theme name="Socle" collapsed="false" id="socle" icon="globe">
            <layer id="l49:vue_adn_suf_majic_2016" name="Bâtiments à raccorder" visible="false" queryable="true" fields="adresse,insee,commune,nb_total,nom_sro,nature,type,proprio" aliases="Adresse,Code INSEE,Commune,Nombre,Nom SRO,Nature,Type,Propriétaire" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:communes" name="Communes" visible="true" url="https://production.metis-map.fr/geoserver/wms" queryable="true" fields="commune,codinsee,dpt,region,epci_2012" aliases="Commune,Code INSEE,Département,Région,EPCI 2012" infoformat="application/vnd.ogc.gml" featurecount="2" />
            <layer id="territoire:cadastre_26_07" name="Cadastre" visible="false" queryable="false" url="https://production.metis-map.fr/geoserver/wms" />
        </theme>

        <theme name="Analyse thématique" collapsed="true" id="analyse_thematique" icon="cogs">
            <layer id="l49:vue_grille_densite_50_nb_suf" name="Grille de densité des bâtiments raccordables" visible="false" queryable="true" fields="nb_suf, radius" aliases="Nombre de bâtiments raccordables,Rayon du cluster" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:vue_cluster_suf" name="Cluster des bâtiments raccordables" visible="false" queryable="true" fields="nb_suf" aliases="Nombre de bâtiments raccordables" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:heatmap" name="Carte de chaleur des bâtiments raccordables" visible="false" queryable="false" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:vue_communes_centroides" name="Proportion de GC à créer par commune" visible="false" queryable="true" fields="emprise_mob" aliases="Nombre total de ml" infoformat="application/vnd.ogc.gml" url="https://production.metis-map.fr/geoserver/wms" />
            <layer id="l49:vue_emprises_mobilisables_26_07" name="Priorisation du GC à créer" visible="false" queryable="false" url="https://production.metis-map.fr/geoserver/wms" />
        </theme>
    </themes>
</config>